{
	"name": "matching-algo",
	"version": "1.0.0",
	"description": "matching algorithm for ranking respondents for a given project",
	"repository": {
		"url": "https://github.com/mrkutly/matching-algo/"
	},
	"main": "dist/start.js",
	"license": "MIT",
	"scripts": {
		"app": "npm run build && npm run start",
		"start": "node dist/start.js",
		"build": "tsc",
		"dev": "tsc-watch --onSuccess \"node dist/start.js\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"format": "prettier --config .prettierrc 'src/**/*.ts' --write"
	},
	"devDependencies": {
		"@tsconfig/node12": "^1.0.7",
		"@types/faker": "^5.1.2",
		"@types/jest": "^26.0.14",
		"@types/node": "^14.11.2",
		"faker": "^5.1.0",
		"jest": "^26.4.2",
		"prettier": "^2.1.2",
		"ts-jest": "^26.4.1",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"csv-parse": "^4.12.0"
	}
}
